    "docs:preview": "vitepress preview docs",
    "watch": "rollup -c -w --bundleConfigAsCjs",
    "compile": "rollup -c --bundleConfigAsCjs --target node",
    "run:app": "electron ./dist/app/background.js",
    "build:mac": "electron-builder -m",
    "build:win": "electron-builder -w",
    "run:all": "pnpm run compile:app & run-p dev watch:app run:app",
